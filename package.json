{
  "name": "instagramstore",
  "version": "1.0.0",
  "description": "InstagramStore - Social Media Management Platform",
  "main": "index.js",
  "scripts": {
    "dev": "node debug-start.js",
    "dev:windows": "debug-start.bat",
    "dev:powershell": "powershell -ExecutionPolicy Bypass -File debug-start.ps1",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "debug:frontend": "cd frontend && npm run dev:debug",
    "debug:backend": "cd backend && npm run debug",
    "debug:backend:break": "cd backend && npm run debug:break",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "test": "cd backend && npm test",
    "lint": "cd frontend && npm run lint",
    "clean": "rm -rf frontend/dist frontend/node_modules backend/node_modules node_modules"
  },
  "keywords": [
    "instagram",
    "social-media",
    "management",
    "react",
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "InstagramStore Team",
  "license": "MIT",
  "devDependencies": {},
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
